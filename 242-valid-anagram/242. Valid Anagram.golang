func isAnagram(s string, t string) bool {
    if len(s) != len(t){
        return false
    }

    counts := make([]int, 26)

    for i, _ := range s{
        counts[s[i]-'a'] += 1
        counts[t[i]-'a'] -= 1
    }

    for _, count := range counts{
        if count != 0{
            return false
        }
    }

    return true
    
}