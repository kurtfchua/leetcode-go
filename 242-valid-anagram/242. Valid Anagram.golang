func isAnagram(s string, t string) bool {
    counts := make([]int, 26)

    for _, c := range s{
        counts[c-'a']++
    }

    for _, c := range t{
        counts[c-'a']--
        if counts[c-'a'] < 0{
            return false
        }
    }

    for _, count := range counts{
        if count != 0{
            return false
        }
    }

    return true
    
}