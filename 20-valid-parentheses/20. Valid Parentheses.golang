func isValid(s string) bool {
    hashmap := map[rune]rune{
        '(':')',
        '[':']',
        '{':'}',
    }

    stack := []rune{}

    for _, c := range s{
        if _, ok := hashmap[c]; ok{
            stack = append(stack, hashmap[c])
        }else if len(stack) > 0 && stack[len(stack)-1] == c{
            stack = stack[:len(stack)-1]
        }else{
            return false
        }
    }

    return len(stack) == 0

}