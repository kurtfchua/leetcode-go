func longestConsecutive(nums []int) int {
    hashset := make(map[int] bool)
    res := 0

    for _, num := range nums{
        hashset[num] = true
    }

    for num, _ := range hashset{
        length := 1
        if !hashset[num-1]{
            for hashset[num+length]{
                length++
            }
            res = max(res, length)
        }
    }
    
    return res
}